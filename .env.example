# List of routers (comma separated IP or Hostname)
ROUTER_HOSTS=192.168.1.1,192.168.1.2

# SSH Port (Default: 22)
PORT=22

# Credentials
JUNIPER_USERNAME=admin
JUNIPER_PASSWORD=secret

# Backup settings
BACKUP_DIR=/backups
MAX_BACKUPS=10

# Timezone (ex: America/Sao_Paulo)
TZ=America/Sao_Paulo

# Notifications (Telegram)
TELEGRAM_BOT_TOKEN=your_bot_token
TELEGRAM_CHAT_ID=your_chat_id